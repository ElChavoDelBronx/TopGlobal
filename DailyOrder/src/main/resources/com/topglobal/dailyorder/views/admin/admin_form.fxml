<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<BorderPane prefHeight="716.0" prefWidth="1350" xmlns="http://javafx.com/javafx/17.0.12" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.topglobal.dailyorder.controllers.admin.AdminFormController"
stylesheets="@../../stylesheets/admin_form.css">
   <center>
      <GridPane alignment="CENTER" BorderPane.alignment="CENTER">
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
           <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
           <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <VBox prefHeight="200.0" prefWidth="100.0">
               <Label text="Gestionar Personal" styleClass="label-view"/>
               <Label text="Editar personal" styleClass="label-welcome"/>
            </VBox>
            <VBox prefHeight="200.0" prefWidth="100.0" GridPane.rowIndex="1">
               <children>
                  <Label alignment="CENTER" prefHeight="18.0" prefWidth="93.0" text="Nombre:*" textAlignment="CENTER" styleClass="label-input"/>
                  <TextField fx:id="tfNombre" maxHeight="-Infinity" maxWidth="-Infinity" prefHeight="50.0" prefWidth="286.0" styleClass="input" />
               </children>
               <GridPane.margin>
                  <Insets />
               </GridPane.margin>
            </VBox>
            <VBox prefHeight="200.0" prefWidth="100.0" GridPane.columnIndex="1" GridPane.rowIndex="1">
               <children>
                  <Label alignment="CENTER" prefHeight="17.0" prefWidth="112.0" text="Primer Apellido:*" styleClass="label-input" />
                  <TextField fx:id="tfApellidoP" maxHeight="-Infinity" maxWidth="-Infinity" prefHeight="50.0" prefWidth="286.0" styleClass="input" />
               </children>
            </VBox>
            <VBox prefHeight="200.0" prefWidth="100.0" GridPane.columnIndex="2" GridPane.rowIndex="1">
               <children>
                  <Label alignment="CENTER" prefHeight="17.0" prefWidth="115.0" text="Segundo Apellido: *" styleClass="label-input" />
                  <TextField fx:id="tfApellidoM" maxHeight="-Infinity" maxWidth="-Infinity" prefHeight="50.0" prefWidth="286.0" styleClass="input" />
               </children>
            </VBox>
            <VBox prefHeight="200.0" prefWidth="100.0" GridPane.columnIndex="3" GridPane.rowIndex="1">
               <children>
                  <Label alignment="CENTER" prefHeight="17.0" prefWidth="56.0" text="Genero:*" styleClass="label-input" />
                  <TextField fx:id="tfGenero" maxWidth="-Infinity" prefHeight="50.0" prefWidth="286.0" styleClass="input" />
               </children>
            </VBox>
            <VBox prefHeight="200.0" prefWidth="100.0" GridPane.rowIndex="2" >
               <children>
                  <Label alignment="CENTER" prefHeight="17.0" prefWidth="142.0" text="Fecha de Nacimiento:*" styleClass="label-input" />
                  <DatePicker fx:id="dpFechaNacimiento" maxWidth="-Infinity" prefHeight="50.0" prefWidth="286.0" styleClass="input" />
               </children>
            </VBox>
            <VBox prefHeight="200.0" prefWidth="100.0" GridPane.columnIndex="1" GridPane.rowIndex="2">
               <children>
                  <Label alignment="CENTER" prefHeight="17.0" prefWidth="70.0" text="Edad:*" styleClass="label-input" />
                  <TextField disable="true" maxWidth="-Infinity" prefHeight="50.0" prefWidth="286.0" styleClass="input" />
               </children>
            </VBox>
            <VBox prefHeight="200.0" prefWidth="100.0" GridPane.columnIndex="2" GridPane.rowIndex="2">
               <children>
                  <Label alignment="CENTER" prefHeight="17.0" prefWidth="66.0" text="CURP:*" styleClass="label-input" />
                  <TextField fx:id="tfCurp" maxWidth="-Infinity" prefHeight="50.0" prefWidth="286.0" styleClass="input" />
               </children>
            </VBox>
            <VBox prefHeight="200.0" prefWidth="100.0" GridPane.columnIndex="3" GridPane.rowIndex="2">
               <children>
                  <Label alignment="CENTER" prefHeight="17.0" prefWidth="138.0" text="Estado de Nacimiento:*" styleClass="label-input" />
                  <ComboBox fx:id="cbEstadoNacimiento" prefHeight="50.0" prefWidth="286.0" styleClass="input" />
               </children>
            </VBox>
            <VBox prefHeight="200.0" prefWidth="100.0" GridPane.rowIndex="3">
