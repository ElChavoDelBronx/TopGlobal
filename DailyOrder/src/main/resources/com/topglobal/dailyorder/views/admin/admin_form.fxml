                  <Label alignment="CENTER" prefHeight="17.0" prefWidth="130.0" text="Correo Electronico:*" styleClass="label-input" />
                  <TextField fx:id="tfEmail" maxWidth="-Infinity" prefHeight="50.0" prefWidth="286.0" styleClass="input" />
               </children>
            </VBox>
            <VBox prefHeight="200.0" prefWidth="100.0" GridPane.columnIndex="1" GridPane.rowIndex="3">
               <children>
                  <Label alignment="CENTER" prefHeight="17.0" prefWidth="84.0" text="Usuario:*" styleClass="label-input" />
                  <TextField fx:id="tfNameUsuario" maxWidth="-Infinity" prefHeight="50.0" prefWidth="286.0" styleClass="input" />
               </children>
            </VBox>
            <HBox prefHeight="100.0" prefWidth="400.0" spacing="16.0" GridPane.columnIndex="3" GridPane.rowIndex="4">
               <children>
                  <Button fx:id="btnCancelar" onAction="#onCancelar" contentDisplay="BOTTOM" mnemonicParsing="false" prefHeight="50.0" prefWidth="185.0" text="Cancelar" />
                  <Button fx:id="btnRegistrar" onAction="#onRegistrar" mnemonicParsing="false" prefHeight="50.0" prefWidth="186.0" text="Registrar" />
               </children>
               <GridPane.margin>
                  <Insets right="20.0" />
               </GridPane.margin>
            </HBox>
            <VBox prefHeight="200.0" prefWidth="100.0" GridPane.columnIndex="2" GridPane.rowIndex="3">
               <children>
                  <Label alignment="CENTER" prefHeight="17.0" prefWidth="139.0" text="Numero de Telefono:*" styleClass="label-input" />
                  <TextField fx:id="tfNumeroTelefono" maxWidth="-Infinity" prefHeight="50.0" prefWidth="286.0" styleClass="input" />
               </children>
            </VBox>
            <VBox prefHeight="200.0" prefWidth="100.0" GridPane.columnIndex="3" GridPane.rowIndex="3">
               <children>
                  <Label alignment="CENTER" prefHeight="17.0" prefWidth="62.0" text="Puesto:*" styleClass="label-input" />
                  <ComboBox fx:id="cbPuesto" prefHeight="50.0" prefWidth="286.0" styleClass="input" />
               </children>
            </VBox>
         </children>
         <padding>
            <Insets bottom="42.0" left="62.0" right="62.0" top="42.0" />
         </padding>
      </GridPane>
